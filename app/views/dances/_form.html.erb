<%= render "layouts/errors", :object => @dance %>

<%= form_for @dance do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name, class: "form-control" %>
  <br>
  <%= f.label :county %>
  <%= f.text_field :county, class: "form-control" %>
  <br>
  <%= f.label :style %>
  <%= f.text_field :style, class: "form-control" %>
  <br>
  <%= f.label :note %>
  <%= f.text_area :note, size: "24x10", class: "form-control" %>
  <br>
  <%= f.submit class: "btn btn-primary" %>
<% end %>
