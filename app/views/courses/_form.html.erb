<%= render "layouts/errors", :object => @course %>

<%= form_for @course do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name, class: "form-control" %>
  <%= f.label :description %>
  <%= f.text_area :description, size: "24x20", class: "form-control" %>
  <br>
  <%= f.submit class: "btn btn-primary" %>
<% end %>
